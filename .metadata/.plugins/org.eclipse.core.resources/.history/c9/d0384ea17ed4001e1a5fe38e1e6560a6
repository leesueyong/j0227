package t01;

public class Test5 {
	public static void main(String[] args) {
		int su = 30;
		System.out.printf("숫자 : %d\n", su);

		int su2 = 50;
		System.out.println("숫자2 : \n"+ 70);
		int su3, su4;
		su3=10;
		su4=20;
		System.out.println("su3 : "+ su3+"\nsu4 : " + su4);
		int _su7=100;
		System.out.println("\nsu7 : "+_su7);
	}
}
